{
  "title": "Function key maps",
  "rules": [
    {
      "description": "Various function remaps",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f1"
          },
          "to": [
            {
              "shell_command": "open -a 'kitty.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f2"
          },
          "to": [
            {
              "shell_command": "open -a 'Firefox.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f3"
          },
          "to": [
            {
              "shell_command": "open -a 'Slack.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f4"
          },
          "to": [
            {
              "shell_command": "open -a 'Microsoft Outlook.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f5"
          },
          "to": [
            {
              "shell_command": "open -a 'Screenshot.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f9"
          },
          "to": [{ "key_code": "a", "modifiers": ["command"] }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f10"
          },
          "to": [{ "key_code": "c", "modifiers": ["command"] }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f11"
          },
          "to": [{ "key_code": "v", "modifiers": ["command"] }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f12"
          },
          "to": [{ "key_code": "f", "modifiers": ["command"] }]
        }
      ]
    }
  ]
}
