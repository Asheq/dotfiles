{
  "title": "Everdo maps",
  "rules": [
    {
      "description": "In Everdo, map Cmd+Enter to Ctrl+Enter",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": ["^net\\.everdo\\.app$"],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": ["command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": ["control"]
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "bundle_identifiers": ["^net\\.everdo\\.app$"],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": ["command"]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
