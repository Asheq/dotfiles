# Work GitHub account
Host github.com
    AddKeysToAgent yes
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_ed25519_work

# Work GitLab account
Host ptc.com
    AddKeysToAgent yes
    HostName ptc.com
    User git
    IdentityFile ~/.ssh/id_ed25519_work

# Personal account
Host github.com-personal
    AddKeysToAgent yes
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_ed25519
