ssource ~/.shell/interactive_functions
ssource ~/.shell/interactive_aliases

# Source POSIX-compliant git scripts
ssource ~/.config/git/git-prompt.sh
ssource ~/.config/git/git-completion.sh
# Preserve completion for git alias, g
__git_complete g __git_main

# Source POSIX-compliant nvm script. Avoid using the slow `nvm use` at
# startup. We should already have a default node available in our PATH.
ssource "$NVM_DIR/nvm.sh" --no-use
